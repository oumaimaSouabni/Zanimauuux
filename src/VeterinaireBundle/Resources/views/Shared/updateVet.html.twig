                         <label class="control-label">Adresse :</label>
                         <div class="controls">
                             <input type="text" class="span11" id="input" placeholder="Adresse " name="adresse"/>
                         </div>
                     </div>
                     <div class="control-group" id="cadre">
                         <label class="control-label">Ville :</label>
                         <div class="controls">
                             <input type="text" class="span11" id="input" placeholder="Ville " name="ville"/>
                         </div>
                     </div>
                     <div class="control-group" id="cadre">
                         <label class="control-label">CodePostale :</label>
                         <div class="controls">
                             <input type="text" class="span11" id="input" placeholder="Code Postale " name="codePostale"/>
                         </div>
                     </div>
                     <div class="control-group" id="cadre">
                         <label class="control-label">Numero d'ordre :</label>
                         <div class="controls">
                             <input type="text" class="span11" id="input" placeholder="Code Postale " name="numeroOrdre"/>
                         </div>
                     </div>


                     <div class="form-actions">
                         <button type="submit" value="submit" class="btn btn-success">Save</button>
                     </div>
                 </form>
             </div>

         </div>
     </div>
 {%endblock%}

