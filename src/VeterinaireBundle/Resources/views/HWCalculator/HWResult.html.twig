            $("#imgResultScale").attr("src","{{ asset('img/scale/result_3.png') }}");
            $('#divDescr3').hide();
            $('#divDescr1').show();
            $('#divDescr2').hide();
        }
        else if(indice > 6 && indice < 9){// surpoid
            if(typeAnimal == 1){
                $("#imgAnimal").attr("src","{{ asset('img/cat/cat_bcs_7.jpg') }}");
            }
            else{
                $("#imgAnimal").attr("src","{{ asset('img/dog/dog_bcs_7.jpg') }}");
            }
            document.getElementById("spanHW").innerHTML = "<h4> Surpoid </h4>";
            $("#imgScale").attr("src","{{ asset('img/scale/scale_7.png') }}");
            $("#imgResultScale").attr("src","{{ asset('img/scale/result_7.png') }}");
            $('#divDescr1').hide();
            $('#divDescr3').show();
            $('#divDescr2').hide();
        }

    }
