            $("#imgResultScale").attr("src","{{ asset('img/scale/result_9.png') }}");
            $('#divDescr1').hide();
            $('#divDescr3').show();
            $('#divDescr2').hide();

        }
        else if(indice > 3 && indice < 6){// animal maigre
            if(typeAnimal == 1){
                $("#imgAnimal").attr("src","{{ asset('img/cat/cat_bcs_3.jpg') }}");
            }
            else{
