<script type="application/javascript">

    function showResult() {

        var radioscote = document.getElementsByName('cote');
        for (var i = 0, length = radioscote.length; i < length; i++)
        {
            if (radioscote[i].checked)
            {
                var cote = radioscote[i].value ;
                break;
            }
        }
        var radiospoid = document.getElementsByName('poid');
        for (var i = 0, length = radiospoid.length; i < length; i++)
        {
            if (radiospoid[i].checked)
            {
                var poid = radiospoid[i].value ;
                break;
            }
        }
        var radiosabdo = document.getElementsByName('abdo');
        for (var i = 0, length = radiosabdo.length; i < length; i++)
        {
            if (radiosabdo[i].checked)
            {
                var  abdo  = radiosabdo[i].value ;
                break;
            }
        }
        document.getElementsByName('hiddenAbdo').value = abdo;
        document.getElementsByName('hiddenCote').value= cote;
        document.getElementsByName('hiddenPoid').value= poid;

        if(abdo == 1 )
            $("#repAbdo").html("<span class=\"badge badge-important\">1</span><b> Abdominoplastie prononcée  </b> ");
        else if(abdo==2)
            $("#repAbdo").html(" <span class=\"badge badge-warning\">2</span><b>   Abdominoplastie proportionnelle  </b> ");
        else if(abdo == 3)
            $("#repAbdo").html(" <span class=\"badge badge-success\">3</span> <b>Abdominoplastie absente </b> ");

        if(cote == 1 )
            $("#repCote").html("<span class=\"badge badge-important\">1</span><b> Ils sont trop importants </b> ");
        if(cote == 2 )
            $("#repCote").html(" <span class=\"badge badge-warning\">2</span><b> Ils sont juste  </b> ");
        if(cote == 3 )
