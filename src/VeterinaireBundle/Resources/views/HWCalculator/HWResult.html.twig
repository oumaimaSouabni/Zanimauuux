        $('#div_questions').hide();
        $('#div_resultat').show();
        ReactDOM.findDOMNode(this).scrollTop = 0;
        return false;
    }
    function  calculerIndiceHW(abdo, cote, poid){

        var indice= parseInt(cote)+ parseInt(abdo) + parseInt(poid);
        var typeAnimal = {{ HWEntity.getTypeAnimal() }};

        if(cote == 2 && abdo == 2 && poid == 2){// parfait
            if(typeAnimal == 1){
                $("#imgAnimal").attr("src","{{ asset('img/cat/cat_bcs_5.jpg') }}");
            }
            else{
                $("#imgAnimal").attr("src","{{ asset('img/dog/dog_bcs_5.jpg') }}");
            }
            $("#imgScale").attr("src","{{ asset('img/scale/scale_5.png') }}");
            $("#imgResultScale").attr("src","{{ asset('img/scale/result_5.png') }}");


            document.getElementById("spanHW").innerHTML = "<h4> Idéal </h4>";



            $('#divDescr3').hide();
            $('#divDescr2').show();
            $('#divDescr1').hide();
        }
        else
        if(cote == 1 && abdo == 1 && poid == 1){//animal tres maigre
            if(typeAnimal == 1){
                $("#imgAnimal").attr("src","{{ asset('img/cat/cat_bcs_1.jpg') }}");
            }
            else{
                $("#imgAnimal").attr("src","{{ asset('img/dog/dog_bcs_1.jpg') }}");
            }
            document.getElementById("spanHW").innerHTML = "<h4> Trés maigre  </h4>";
            $("#imgScale").attr("src","{{ asset('img/scale/scale_1.png') }}");
            $("#imgResultScale").attr("src","{{ asset('img/scale/result_1.png') }}");
            $('#divDescr3').hide();
            $('#divDescr1').show();
            $('#divDescr2').hide();
        }
        else if(indice == 9){ // tres obese
            if(typeAnimal == 1){
                $("#imgAnimal").attr("src","{{ asset('img/cat/cat_bcs_9.jpg') }}");
            }
            else{
                $("#imgAnimal").attr("src","{{ asset('img/dog/dog_bcs_9.jpg') }}");
            }
            document.getElementById("spanHW").innerHTML = "<h4> Trés obèse </h4>";

