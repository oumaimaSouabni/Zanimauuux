{% endblock %}
 {% block contenu %}

     <div id="content">
         <!--breadcrumbs-->
         <div id="content-header">
             <div id="breadcrumb"> <a href="{{ path('loginadmin') }}" title="Go to Home" class="tip-bottom"><i class="icon-home"></i> Home</a></div>
         </div>
         <div class="widget-box">
             <div class="widget-title"> <span class="icon"> <i class="icon-align-justify"></i> </span>
                 <h5>Formulaire cabinet</h5>
             </div>
             <div class="widget-content nopadding" >
                 <form action="#" method="POST" class="form-horizontal" >
                     <div class="control-group" id="cadre">
                         <label class="control-label">Email Cabinet:</label>
                         <div class="controls">
                             <input type="text" class="span11" id="input" placeholder="Email de votre cabinet"
                                    name="emailcabinet"/>
                         </div>
                     </div>
                     <div class="control-group" id="cadre">
                         <label class="control-label">Immatricule Cabinet:</label>
                         <div class="controls">
                             <input type="text" class="span11" id="input" placeholder="Immatricule cabinet " name="immatriculecabinet"/>
                         </div>
                     </div>
                     <div class="control-group" id="cadre">
                         <label class="control-label">Telephone Cabinet:</label>
                         <div class="controls">
                             <input type="text" class="span11" id="input" placeholder="Telephone " name="telephonecabinet"/>
                         </div>
                     </div>
                     <div class="control-group" id="cadre">
                         <label class="control-label">Fax Cabinet:</label>
                         <div class="controls">
                             <input type="text" class="span11" id="input" placeholder="Fax " name="faxcabinet"/>
                         </div>
                     </div>
                     <div class="control-group" id="cadre">
                         <label class="control-label">Adresse Cabinet:</label>
                         <div class="controls">
                             <input type="text" class="span11" id="input" placeholder="Adresse " name="adressecabinet"/>
                         </div>
                     </div>
                     <div class="control-group" id="cadre">
                         <label class="control-label">Ville Cabinet:</label>
                         <div class="controls">
                             <input type="text" class="span11" id="input" placeholder="Ville " name="villecabinet"/>
                         </div>
                     </div>
                     <div class="control-group" id="cadre">
                         <label class="control-label">CodePostale Cabinet:</label>
                         <div class="controls">
                             <input type="text" class="span11" id="input" placeholder="Code Postale " name="codepostalecabinet"/>
                         </div>
                     </div>

                     <div class="control-group" id="cadre">
                         <label class="control-label">Image Cabinet:</label>
                         <div class="controls">
                             <input type="file" name="photovet"/>
                         </div>
                     </div>
                     <div class="form-actions">
                         <button type="submit" value="submit" class="btn btn-success">Save</button>
                     </div>
                 </form>
             </div>

         </div>
     </div>
 {%endblock%}

{% block script %}

    <script src="{{ asset('js/jquery.min.js')}}"></script>
