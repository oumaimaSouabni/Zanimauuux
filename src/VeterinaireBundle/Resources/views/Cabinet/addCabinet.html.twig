    <link href="{{ asset('adminpage/css/formulaire.css')}}" rel="stylesheet" />

{% endblock %}
{% block bar_laterale %}

    <div id="sidebar"><a href="{{ path('loginadmin') }}" class="visible-phone"><i class="icon icon-home"></i> Dashboard</a>
        <ul class="nav">
            <li ><a href="{{ path('affiche') }}"><i class="icon icon-home"></i> <span> Dashboard</span></a> </li>
            <li class="submenu"> <a href="#"><i class="icon icon-th-list"></i> <span> Gestion Articles</span> <span class="label label-important">2</span></a>
                <ul>
                    <li class="active"> <a href="#"><i class="icon icon-signal"></i> <span> Ajout Article</span></a> </li>
